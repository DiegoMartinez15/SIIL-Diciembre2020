eld\r\n          v-model=\"searchEgresado\"\r\n          append-icon=\"search\"\r\n          label=\"Buscar egresado\"\r\n          single-line\r\n          hide-details\r\n        ></v-text-field>\r\n      </v-card-title>\r\n      <v-data-table\r\n        \r\n        :headers=\"hTBegresado\"\r\n        :items=\"arrayEgresado\"\r\n        :footer-props=\"{\r\n          'items-per-page-options' :[5,10,15,20,30],\r\n          'items-per-page-text' :'Registros por Página'\r\n        }\"\r\n        :items-per-page=\"5\"\r\n        :search=\"searchEgresado\"\r\n        multi-sort\r\n        class=\"elevation-3 text--center ml-80 pl-10 pr-10\"\r\n      >\r\n      <!-- Templeate para form modal para agregar o actualizar categorias-->\r\n    <template v-slot:top>\r\n      <v-toolbar flat color=\"white\">\r\n        \r\n        <div class=\"flex-grow-1\"></div>\r\n        <v-dialog v-model=\"modalSeguimiento\" persistent max-width=\"700px\">\r\n          <v-card>\r\n            <v-card-title class=\"headline light-blue lighten-5\" primary-titles>\r\n              <span class=\"headline\" v-text=\"Seguimiento a Graduados\"></span>\r\n            </v-card-title>\r\n            <v-card-text>\r\n              <v-container>\r\n                <v-form ref=\"formSeguimiento\" v-model=\"validForm\" :lazy-validation=\"true\">\r\n                <v-text-field\r\n                  append-icon=\"mdi-folder-outline\"\r\n                  v-model=\"areas.nombre\"\r\n                  @keyup=\"errorsNombre = []\"\r\n                  :rules=\"[v => !!v || 'Nombre Es Requerido']\"\r\n                  label=\"Nombre\"\r\n                  required\r\n                  :error-messages=\"errorsNombre\"\r\n                  ></v-text-field>  \r\n                </v-form>\r\n              </v-container>\r\n            </v-card-text>\r\n            <v-divider></v-divider>           \r\n              <v-card-actions>\r\n                <div class=\"flex-grow-1\"></div>\r\n                <v-btn color=\"red darken-1\" text @click=\"cerrarModal\">Cerrar</v-btn>\r\n                <v-btn\r\n                  color=\"info darken-1\"\r\n                  :disabled=\"!validForm\"\r\n                  @click=\"saveForm()\"\r\n                  text\r\n                  v-text=\"Enviar\"\r\n                ></v-btn>\r\n              </v-card-actions>          \r\n          </v-card>\r\n        </v-dialog>\r\n      </v-toolbar>\r\n</template>\r\n      <!--Template para la columna de acciones -->\r\n      <template v-slot:item.action=\"{item}\">\r\n        <v-tooltip top>\r\n          <template v-slot:activator=\"{on}\">\r\n            <v-btn\r\n              color=\"success\"            \r\n              elevation=\"8\"\r\n              small\r\n              fab\r\n              dark\r\n              :disabled=\"item.id<0\"\r\n              v-on=\"on\"\r\n              @click=\"modalSeguimiento=true\"\r\n            >\r\n            <v-icon>create</v-icon>\r\n            </v-btn>\r\n          </template>\r\n          <span>Seguimiento a Graduado</span>\r\n        </v-tooltip>\r\n      </template>\r\n      <v-snackbar v-model=\"snackbar\">\r\n          {{ msjSnackBar}}\r\n          <v-btn color=\"red\" text @click=\"snackbar = false\">Cerrar</v-btn>\r\n      </v-snackbar>\r\n      </v-data-table>\r\n    </v-card>\r\n  </div>\r\n</div>\r\n</template>\r\n<script>\r\n  export default {\r\n    data () {\r\n      return {\r\n        arrayEgresado:[],\r\n        hTBegresado:[\r\n          {text:\"Nombres\", value:'nombre',class:'blue-grey lighten-4'},\r\n          {text:\"Apellidos\", value:'apellido',class:'blue-grey lighten-4'},\r\n          {text:\"Antiguo Número de Teléfono\", value:'telefono',class:'blue-grey lighten-4 '},\r\n          {text:\"Nuevo Número de Teléfono\", value:'celular2',class:'blue-grey lighten-4 '},\r\n          {text:\"Carrera\", value:'carrera', class:'blue-grey lighten-4 '},\r\n          {text:\"Correo\", value:'correo', class:'blue-grey lighten-4 '},\r\n          {text:\"Acciones\", value:'action',sortable:false,aling:\"center\",class:'blue-grey lighten-4'},\r\n        ],\r\n        loader:false,\r\n        searchEgresado:\"\",\r\n        modalSeguimiento:false,\r\n        validForm:true,\r\n        formulario:{\r\n          id_aspirante:\"\",\r\n          id_egresado:\"\",\r\n          continua_estudiando:\"\",\r\n          empleado:\"\",\r\n          empleado_area_trabajo:\"\",\r\n          interes_colocado:\"\",\r\n          tipo_empleo:\"\",\r\n          nombre_empresa:\"\",\r\n          contacto_empresa:\"\",\r\n          cargo_desempenia:\"\",\r\n          tel_empresa:\"\",\r\n          estatus:\"\",\r\n          fecha_monitoreo:\"\"\r\n        },\r\n        yesno: ['Si', 'No'],\r\n        snackbar:false,\r\n        msjSnackBar:\"\",\r\n        errorsNombre:[],\r\n        \r\n        \r\n      };\r\n    },\r\n    computed: {\r\n     \r\n    },\r\n    \r\n    methods: {\r\n      fetchEgresados() {  \r\n        let token = sessionStorage.getItem('tokenS');\r\n        let me = this,\r\n         header = {\r\n              headers: {\r\n                \"Authorization\": \"Bearer \"+ token,\r\n                \r\n              },\r\n            };\r\n               \r\n        me.loader = true;\r\n        me.$http.get(`${me.$url}/formularioSeguimiento`,header)\r\n        .then(function(response){\r\n            console.log(response.data)\r\n          me.arrayEgresado = response.data;\r\n          me.loader = false;\r\n        })\r\n        .catch(function(error){\r\n          me.loader = false;\r\n          console.log(error);\r\n        });\r\n      },\r\n      cerrarModal(){\r\n        let me = this;\r\n        me.modalSeguimiento = false;\r\n        setTimeout(() => {\r\n          me.formulario={\r\n          id_aspirante:\"\",\r\n          id_egresado:\"\",\r\n          continua_estudiando:\"\",\r\n          empleado:\"\",\r\n          empleado_area_trabajo:\"\",\r\n          interes_colocado:\"\",\r\n          tipo_empleo:\"\",\r\n          nombre_empresa:\"\",\r\n          contacto_empresa:\"\",\r\n          cargo_desempenia:\"\",\r\n          tel_empresa:\"\",\r\n          estatus:\"\",\r\n          fecha_monitoreo:\"\"\r\n          };\r\n          me.resetValidation();\r\n        }, 300);\r\n      },\r\n      resetValidation() {\r\n        let me = this;\r\n        me.errorsNombre = [];\r\n        me.$refs.formSeguimientoform.resetValidation();\r\n      },\r\n      saveForm(){\r\n        let token = localStorage.getItem('token');\r\n        let me = this,\r\n         header = {\r\n              headers: {\r\n                \"Authorization\": \"Bearer \"+ token,\r\n                \r\n              },\r\n            };\r\n         \r\n          if (me.$refs.formPerfil.validate()) {\r\n             const Toast = me.$swal.mixin({\r\n              timer: 10000,\r\n               timerProgressBar: true,   \r\n          \r\n            });\r\n               Toast.fire({\r\n              width:'200%',\r\n              padding:'2rem',\r\n              iconColor:'#814690',\r\n              backdrop:true,\r\n              title: 'Enviar la Informacion?',\r\n              text: \"Estas a punto de enviar esta informacion!\",\r\n              icon: 'warning',\r\n              showCancelButton: true,\r\n              confirmButtonColor: '#3085d6',\r\n              cancelButtonColor: '#d33',\r\n              confirmButtonText: 'Enviar información',\r\n              cancelButtonText: 'Cancelar'\r\n            }).then((result) => {\r\n              if (result.isConfirmed) {\r\n               me.formulario.id_aspirante = me.arrayAspirante.id;\r\n               me.formulario.id_egresado = me.arrayAlumno.id;\r\n              console.log(me.formulario.id_aspirante)\r\n              console.log(me.formulario.id_egresado)\r\n              console.log(me.formulario)\r\n              \r\n              me.$http.post(`${me.$url}/seguimientoSave`, me.formulario, header)\r\n                .then(function(response) {\r\n                  if(response.status == 200){\r\n                    me.loader = true;\r\n                  Toast.fire(\r\n                  'Enviada Correctamente!',\r\n                  'Información enviada con éxito',\r\n                  'success'\r\n                  );\r\n                  me.loader = false;\r\n                  }\r\n               \r\n              })\r\n              .catch(function(error) {\r\n                console.log(error);\r\n                me.$swal(\"Error\", \"Ocurrio Un Error Intente Nuevamente\", \"error\");\r\n              });\r\n               \r\n              }\r\n            });\r\n           \r\n          }     \r\n      },\r\n      saveFormulario() {\r\n      let me = this;\r\n      const Toast = me.$swal.mixin({\r\n          toast: true,\r\n          position: \"bottom-end\",\r\n          showConfirmButton: true,\r\n          timer: 3000\r\n        });\r\n      Toast.fire({\r\n              icon: \"success\",\r\n              title: \"Informacion enviada correctamente\"\r\n            });\r\n           me.$http.post(`${me.$url}/seguimientoSave`, me.formulario)\r\n            .then(function(response) {\r\n            me.cerrarModal();\r\n          })\r\n          .catch(function(error) {\r\n           console.log(error);\r\n            me.$swal(\"Error\", \"Ocurrio Un Error Intente Nuevamente\", \"error\");\r\n            me.loader = false;\r\n          });\r\n      \r\n    },\r\n      setMessageToSnackBar(msj, estado) {\r\n        let me = this;\r\n        me.snackbar = estado;\r\n        me.msjSnackBar = msj;\r\n      },\r\n      \r\n      verificarAccionDato(areas, statusCode, accion) {\r\n        let me = this;\r\n\r\n        const Toast = me.$swal.mixin({\r\n          toast: true,\r\n          position: \"bottom-end\",\r\n          showConfirmButton: true,\r\n          timer: 3000\r\n        });\r\n\r\n        switch (accion) {\r\n          case \"add\":\r\n            //Agrego al