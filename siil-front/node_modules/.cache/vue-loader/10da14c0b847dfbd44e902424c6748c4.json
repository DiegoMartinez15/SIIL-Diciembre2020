-list-item-content>\r\n                <v-list-item-title>Estad&iacute;sticas</v-list-item-title>\r\n              </v-list-item-content>\r\n          \r\n        </template>\r\n        <!--Menu de categoria-->\r\n\r\n        <v-list-item link :to=\"{path:'#'}\">\r\n              <v-list-item-action>\r\n                <v-icon>multiline_chart</v-icon>\r\n              </v-list-item-action>\r\n              <v-list-item-content>\r\n                <v-list-item-title>Estad&iacute;stica 1</v-list-item-title>\r\n              </v-list-item-content>\r\n         </v-list-item>  \r\n            <!--MENU DE MARCAS-->\r\n            <v-list-item link :to=\"{path:'#'}\"><!--ESTA FORMA FUNCIONA BIEN ASI SE DECLARAN LAS RUTAS-->\r\n              <v-list-item-action>\r\n                <v-icon>trending_up</v-icon>\r\n              </v-list-item-action>\r\n              <v-list-item-content>\r\n                <v-list-item-title>Estad&iacute;stica 2</v-list-item-title>\r\n              </v-list-item-content>\r\n            </v-list-item>      \r\n            <!--Menu de Producto--> \r\n            <v-list-item link :to=\"{path: '#'}\">\r\n              <v-list-item-action>\r\n                <v-icon>bubble_chart</v-icon>\r\n              </v-list-item-action>\r\n              <v-list-item-content>\r\n                <v-list-item-title>Estad&iacute;stica 3</v-list-item-title>\r\n              </v-list-item-content>\r\n            </v-list-item> \r\n        </v-list-group>\r\n\r\n<v-divider></v-divider>\r\n        <template>\r\n          <v-list-item class=\"mt-2 mb-2\">\r\n              <v-list-item-action>\r\n                <v-icon>exit_to_app</v-icon>\r\n              </v-list-item-action>\r\n              <v-list-item-content style=\"margin-left:-20px\">\r\n                <v-list-item-title><v-btn style=\"border:2px solid white\" color=\"primary\" @click=\"logout()\">\r\n                 Cerrar Sesion</v-btn></v-list-item-title>\r\n              </v-list-item-content>\r\n    